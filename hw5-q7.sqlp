USE hw5;
SELECT DISTINCT Y.`-car_code` AS country_code, Y.name AS country_name, Z AS seas
FROM world X, X.mondial.country Y
LET Z = (SELECT S.name AS sea
		 FROM world X2, X2.mondial.sea S, split(S.`-country`, " ") R
		 WHERE Y.`-car_code` = R)
WHERE array_count(Z) >= 2
ORDER BY array_count(Z) DESC;

